<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="02" author="geo">
        <createTable tableName="geo"
                     remarks="A table to contain all the geo">
            <column name="id" type="int" autoIncrement="true">
                <constraints nullable="true" primaryKey="true"/>
            </column>
            <column name="lat" type="double precision">
                <constraints nullable="false"/>
            </column>
            <column name="lng" type="double precision">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
